<view class="cu-bar tabbar" style='position:absolute;bottom:0;width:100%'>
  <view class="action text-blue">
    <view class='icon-homefill'></view> 首页
  </view>
  <view class="action text-gray">
    <view class='icon-similar'></view> 分类
  </view>
  <view class="action text-gray add-action">
    <button class='cu-btn icon-add bg-blue shadow' bindtap="showModal" data-target="ChooseModal"></button>
    发布
  </view>
  <view class="action text-gray">
    <view class='icon-cart'>
      <view class='cu-tag badge'>99</view>
    </view>
    购物车
  </view>
  <view class="action text-gray">
    <view class='icon-my'>
      <view class='cu-tag badge'></view>
    </view>
    我的
  </view>
</view>

<view class="cu-modal bottom-modal {{modalName=='ChooseModal'?'show':''}}" bindtap='hideModal'>

  <view class="cu-dialog" catchtap>
    <view class="cu-bar bg-white">
      <view class='action text-green'>确定</view>
      <view class='action text-blue' bindtap='hideModal'>取消</view>
    </view>
    <scroll-view scroll-x class="bg-white nav text-center">
      <view class="cu-item {{index==TabCur?'text-blue cur':''}}" wx:for="{{type}}" wx:key bindtap='tabSelect' data-id="{{index}}">
        {{item}}
      </view>
    </scroll-view>

    <view class="cu-list grid col-4">
      <view class="cu-item" wx:for="{{icon}}" wx:key wx:if="{{item.isShow}}">
        <text class='icon-{{item.name}} lg text-gray'></text>
        <text>{{item.name}}</text>
      </view>
    </view>

    <view class="bg-white radius shadow-blur" style='font-size: 26px;width:92%;height:56px;text-align:right;margin:4%;'>￥{{screenNum}}</view>
    <view class='bg-blue radius text-center light' style='width:92%;height:34px;margin:4%;'>
      <button class='cu-btn bg-blue shadow fl' style='margin:5px;height:48rpx;'>现金</button>
      <button class='cu-btn bg-blue shadow fl' style='margin:5px;height:48rpx;'>今天</button>
      <text class='icon-pic lg text-gray fr' style='font-size: 24px;height: 34px;line-height: 34px;margin-left:10px;margin-right:10px'></text>
      <text class='icon-edit lg text-gray fr' style='font-size: 21px;height: 34px;line-height: 34px;'></text>
    </view>
    <view class="content" style='width:100%;height:248px'>
      <view class="grid col-4 padding-sm">
        <view class="bg-white padding radius text-center shadow-blur" bindtap="btnClick" id="{{n7}}" style='font-size: 26px;'>7</view>
        <view class="bg-white padding radius text-center shadow-blur" bindtap="btnClick" id="{{n8}}" style='font-size: 26px;'>8</view>
        <view class="bg-white padding radius text-center shadow-blur" bindtap="btnClick" id="{{n9}}" style='font-size: 26px;'>9</view>
        <view class="bg-white padding radius text-center shadow-blur" bindtap="btnClick" id="{{na}}" style='font-size: 26px;'>
          <text class='icon-back_android lg text-gray'></text>
        </view>

        <view class="bg-white padding radius text-center shadow-blur" bindtap="btnClick" id="{{n4}}" style='font-size: 26px;'>4</view>
        <view class="bg-white padding radius text-center shadow-blur" bindtap="btnClick" id="{{n5}}" style='font-size: 26px;'>5</view>
        <view class="bg-white padding radius text-center shadow-blur" bindtap="btnClick" id="{{n6}}" style='font-size: 26px;'>6</view>
        <view class="bg-white padding radius text-center shadow-blur" bindtap="btnClick" id="{{na}}" style='font-size: 26px;'>
          <text class='icon-add lg text-gray'></text>
        </view>

        <view class="bg-white padding radius text-center shadow-blur" bindtap="btnClick" id="{{n1}}" style='font-size: 26px;'>1</view>
        <view class="bg-white padding radius text-center shadow-blur" bindtap="btnClick" id="{{n2}}" style='font-size: 26px;'>2</view>
        <view class="bg-white padding radius text-center shadow-blur" bindtap="btnClick" id="{{n3}}" style='font-size: 26px;'>3</view>
        <view class="bg-white padding radius text-center shadow-blur" bindtap="btnClick" id="{{nb}}" style='font-size: 26px;'>
          <text class='icon-move lg text-gray'></text>
        </view>

        <view class="bg-white padding radius text-center shadow-blur" bindtap="btnClick" id="{{n0}}" style='font-size: 26px;'>.</view>
        <view class="bg-white padding radius text-center shadow-blur" bindtap="btnClick" id="{{n0}}" style='font-size: 26px;'>0</view>
        <view class="bg-white padding radius text-center shadow-blur" bindtap="btnJs" style='font-size: 26px;'>
          <text class='icon-post lg text-gray'></text>
        </view>
        <view class="bg-white padding radius text-center shadow-blur" bindtap="btnClick" id="{{nd}}" style='font-size: 26px;'>
          <text class='icon-check lg text-gray'></text>
        </view>
      </view>
    </view>
    <!-- <view class='grid col-3 padding-sm'>
      <view wx:for="{{9}}" class='padding-xs' wx:key>
        <button class='cu-btn orange lg block {{index == 2?"bg":"line"}}-orange'>Item {{index + 1}}</button>
      </view>
    </view> -->
  </view>
</view>